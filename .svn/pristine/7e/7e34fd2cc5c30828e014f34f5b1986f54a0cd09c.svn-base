<?xml version="1.0" encoding="utf-8"?>
<com.yhrjkf.woyundong.view.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/graybg"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <RelativeLayout
        android:id="@+id/am_rl_topbar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_widthPercent="100%"
        app:layout_heightPercent="7%"
        android:background="@color/bluefont">
        <ImageView
            android:layout_width="15dp"
            android:src="@drawable/leftarrow"
            android:layout_centerVertical="true"
            android:id="@+id/am_img_back"
            android:layout_marginLeft="10dp"
            android:layout_height="15dp" />
        <TextView
            android:id="@+id/am_tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginLeft="5dp"
            android:text="奖章"
            android:textColor="@color/white"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/am_tv_rule"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="规则"
            android:textColor="@color/white"
            android:textSize="16sp" />
    </RelativeLayout>
    <LinearLayout
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="15dp"
            android:background="@color/white"
            android:layout_height="0dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_height="20dp">
                <View
                    android:layout_width="5dp"
                    android:background="@color/level"
                    android:layout_height="match_parent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:text="运动次数奖章"
                    android:textSize="16sp"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/grayfont"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_height="0dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_goodtwo"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_qi"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_jianchi"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:layout_gravity="center"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_xiguan"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_goodtwo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:textColor="@color/grayfont"
                            android:gravity="center"
                            android:text="好事成双"
                            android:textSize="14dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_qi"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="幸运七"
                            android:background="@drawable/medal_gray_style"
                            android:textColor="@color/grayfont"
                            android:textSize="14dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_jianchi"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:gravity="center"
                            android:text="坚持运动"
                            android:textColor="@color/grayfont"
                            android:textSize="14dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_xiguan"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:gravity="center"
                            android:text="习惯运动"
                            android:textColor="@color/grayfont"
                            android:textSize="14dp" />
                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_aishang"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_60"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_90"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >

                        <ImageView
                            android:id="@+id/am_img_medal_120"
                            android:layout_width="60dp"
                            android:layout_height="70dp"
                            android:background="@drawable/medal_gray"
                            android:padding="10dp" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_aishang"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:gravity="center"
                            android:text="爱上运动"
                            android:textColor="@color/grayfont"
                            android:textSize="14dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_60"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:gravity="center"
                            android:text="60次运动"
                            android:textColor="@color/grayfont"
                            android:textSize="14dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_90"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:gravity="center"
                            android:text="90次运动"
                            android:textColor="@color/grayfont"
                            android:textSize="14dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/am_tv_medal_120"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/medal_gray_style"
                            android:textColor="@color/grayfont"
                            android:gravity="center"
                            android:text="120次运动"
                            android:textSize="12dp" />
                    </LinearLayout>


                </LinearLayout>
            </LinearLayout>
        </LinearLayout>



