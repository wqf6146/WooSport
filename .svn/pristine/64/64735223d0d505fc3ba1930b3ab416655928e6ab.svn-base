<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <RelativeLayout 
        android:id="@+id/ll_set_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="#F6F6F8"
        >
        <LinearLayout 
            android:id="@+id/ll_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_centerVertical="true"
            android:gravity="center"
            >
        <ImageView 
            android:id="@+id/img_back"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/back2"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textSize="18dp"
            android:text="我的"
            android:textColor="#93D7F6"
            />
        </LinearLayout>
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="记录"
            android:textSize="18dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textColor="#000000"
            />
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000"
            />
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
        <LinearLayout 
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EFEFF4"
            >
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/longtime"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="时间最长"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EAECEE"
            >
        <TextView 
            android:id="@+id/tv_jilu_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2000秒"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
         <TextView 
             android:id="@+id/tv_jilu_shijian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2009-16-24 19:20:34创建"
            android:textColor="#AAAAAA"
           
            />
        </LinearLayout>
        </LinearLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000"
            />
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
        <LinearLayout 
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EFEFF4"
            >
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/longjuli"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="距离最远"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EAECEE"
            >
        <TextView 
            android:id="@+id/tv_jilu_juli"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="17485米"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
         <TextView 
            android:id="@+id/tv_jilu_shijian1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2009-16-24 19:20:34创建"
            android:textColor="#AAAAAA"
            />
        </LinearLayout>
        </LinearLayout>
         <View 
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000"
            />
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
        <LinearLayout 
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EFEFF4"
            >
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ranshao"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="燃烧最多"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EAECEE"
            >
        <TextView 
            android:id="@+id/tv_jilu_ranshao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="25千卡"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
         <TextView 
            android:id="@+id/tv_jilu_shijian2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2009-16-24 19:20:34创建"
            android:textColor="#AAAAAA"
            />
        </LinearLayout>
        </LinearLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000"
            />
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
        <LinearLayout 
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EFEFF4"
            >
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/sudu"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="速度最快"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="#EAECEE"
            >
        <TextView 
            android:id="@+id/tv_jilu_sudu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="47.6 m/h"
            android:textSize="18dp"
            android:textColor="#FF0000"
            />
         <TextView 
            android:id="@+id/tv_jilu_shijian3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2009-16-24 19:20:34创建"
            android:textColor="#AAAAAA"
            />
        </LinearLayout>
        </LinearLayout>
    

</LinearLayout>
