<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout 
        android:id="@+id/ll_set_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:gravity="center_vertical"
        android:background="#F6F6F8"
        >
        <ImageView 
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/back2"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textSize="18dp"
            android:text="我的"
            android:textColor="#93D7F6"
            />
    </LinearLayout>
    <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        />
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical"
            >
            <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        />
             <View 
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#EFEFF4"
        />
            <RelativeLayout 
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingLeft="30dp"
                android:background="#FFFFFF"
                 android:paddingBottom="10dp"
                 android:id="@+id/ll_set_yyts"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="语音提示"
                    android:layout_centerVertical="true"
                    android:textColor="#000000"
                    />
                <CheckBox
                    android:id="@+id/cheb_set"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>
      <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        android:layout_marginLeft="30dp"
        />
             <TextView 
                 android:id="@+id/tv_set_tbydsj"
                 android:layout_width="match_parent"
                 android:layout_height="50dp"
                 android:text="同步云端数据"
                 android:paddingLeft="30dp"
                 android:gravity="center_vertical"
                 android:textColor="#000000"
                 />
             <View 
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#EFEFF4"
        />
             <RelativeLayout 
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingLeft="30dp"
                android:background="#FFFFFF"
                 android:id="@+id/rl_set_rjbb"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="软件版本"
                    android:layout_centerVertical="true"
                    android:textColor="#000000"
                    />
                 <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="V1.0"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:textColor="#CCCCCC"
                    android:layout_marginRight="20dp"
                    android:id="@+id/tv_set_bb"
                    />
            </RelativeLayout>
              <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        android:layout_marginLeft="30dp"
        />
              <RelativeLayout 
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingLeft="30dp"
                android:background="#FFFFFF"
                 android:id="@+id/rl_set_yjfk"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="意见反馈"
                    android:layout_centerVertical="true"
                    android:textColor="#000000"
                    />
                <ImageView 
                    android:layout_width="15dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/backright"
                    />
            </RelativeLayout>
             <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        android:layout_marginLeft="30dp"
        />
              <RelativeLayout 
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingLeft="30dp"
                android:background="#FFFFFF"
                 android:id="@+id/rl_set_cpjs"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="产品介绍"
                    android:layout_centerVertical="true"
                    android:textColor="#000000"
                    />
                <ImageView 
                    android:layout_width="15dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/backright"
                    />
            </RelativeLayout>
             <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        android:layout_marginLeft="30dp"
        />
              <RelativeLayout 
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingLeft="30dp"
                android:background="#FFFFFF"
                 android:id="@+id/rl_set_gywm"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="关于我们"
                    android:layout_centerVertical="true"
                    android:textColor="#000000"
                    />
                <ImageView 
                    android:layout_width="15dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/backright"
                    />
            </RelativeLayout>
            <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/viewc"
        android:layout_marginLeft="30dp"
        />
            <RelativeLayout 
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingLeft="30dp"
                android:background="#FFFFFF"
                 android:id="@+id/rl_set_jszc"
                >
                <TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="技术支持"
                    android:layout_centerVertical="true"
                    android:textColor="#000000"
                    />
                <ImageView 
                    android:layout_width="15dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/backright"
                    />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
    

</LinearLayout>
